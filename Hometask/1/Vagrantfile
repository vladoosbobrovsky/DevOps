# -*- mode: ruby -*-# vi: set ft=ruby :Vagrant.configure(2) do |config|   config.vm.box = "centos/7"   config.vm.hostname = "myhost"   config.vm.network "forwarded_port", guest: 80, host: 8080, host_ip: "127.0.0.1"   config.vm.synced_folder "src/", "/var/www/html"   end   	config.vm.provider "virtualbox" do |v|    v.memory = 512    v.cpus = 2    config.vm.provision "shell", path: "script.sh"end